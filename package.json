{
  "name": "tenga",
  "version": "1.1.0",
  "description": "Professional laundry service mobile application for Tenga Laundry platform",
  "author": "Tenga Laundry Team",
  "license": "MIT",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "start:tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:proxy": "node scripts/dev-proxy.js",
    "web:test": "node scripts/test-proxy.js",
    "web:dev": "concurrently \"npm run web:proxy\" \"npm run web\"",
    "web:build": "expo export --platform web",
    "build": "npm run web:build",
    "build:android": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "build:all": "eas build --platform all --profile production",
    "build:preview": "eas build --platform all --profile preview",
    "build:local": "expo build:android -d",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "update": "eas update",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clear-cache": "expo start --clear",
    "reset": "expo start --clear && npx react-native reset-cache",
    "clean": "rm -rf node_modules && npm install && expo start --clear",
    "optimize": "npx react-native-bundle-visualizer",
    "validate-config": "node scripts/validate-config.js",
    "diagnose-env": "node scripts/diagnose-env.js",
    "test-runtime-config": "node scripts/test-runtime-config.js",
    "setup:production": "node scripts/setup-production.js",
    "start:production": "NODE_ENV=production expo start"
  },
  "dependencies": {
    "@expo/metro-config": "~0.19.0",
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/material-top-tabs": "^7.0.11",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@tanstack/react-query": "^5.80.7",
    "axios": "^1.12.2",
    "base64-arraybuffer": "^1.0.2",
    "date-fns": "^4.1.0",
    "deprecated-react-native-prop-types": "^5.0.0",
    "dotenv": "^16.6.1",
    "expo": "~52.0.0",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-location": "~18.0.10",
    "expo-modules-core": "~2.0.4",
    "expo-router": "~4.0.21",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.30.4",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.4",
    "geokit": "^1.1.0",
    "node-fetch": "^3.3.2",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "1.18.0",
    "react-native-pager-view": "6.5.1",
    "react-native-ratings": "^8.1.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-tab-view": "^3.5.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.2",
    "yup": "^1.7.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-arrow-functions": "^7.27.1",
    "@babel/plugin-transform-shorthand-properties": "^7.27.1",
    "@babel/plugin-transform-template-literals": "^7.27.1",
    "@react-native/babel-preset": "^0.79.3",
    "@types/prop-types": "^15",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "jest-expo": "~52.0.6",
    "lint-staged": "^13.0.0",
    "nativewind": "^4.2.1",
    "postcss": "^8.5.6",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "schema-utils": {
      "ajv": "^8.12.0",
      "ajv-keywords": "^5.1.0"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tengalaundry/mobile-app.git"
  },
  "bugs": {
    "url": "https://github.com/tengalaundry/mobile-app/issues"
  },
  
    "optionalDependencies": {
      "lightningcss-linux-x64-gnu": "^1.21.0"
    },
  
  "homepage": "https://tengalaundry.com",
  "keywords": [
    "laundry",
    "mobile-app",
    "react-native",
    "expo",
    "typescript",
    "supabase",
    "tanzania",
    "service-app"
  ]
 
}
